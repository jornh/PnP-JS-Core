<header class="ms-Grid">
    <div class="ms-Grid-row top-row">
        <div class="ms-Grid-col ms-u-sm11 ms-u-md9 ms-u-lg7 ms-u-xl5 ms-u-xxl4">
            <a href="{{relativeURL "index.html"}}" class="title ms-font-xxl ms-fontColor-white" style="text-decoration:none">{{project.name}}</a>
        </div>
        <div class="ms-Grid-col ms-u-sm1 ms-u-hiddenMdUp">
            <span class="ms-font-xxl ms-fontColor-white"><i class="ms-Icon ms-Icon--menu2" aria-hidden="true"></i></span>            
        </div>

        <div class="ms-Grid-col ms-u-md1 ms-u-lg1 ms-u-xxl3 ms-u-hiddenSmDown">
            <span class="ms-font-xxl ms-fontColor-white"><i class="ms-Icon ms-Icon--download" aria-hidden="true"></i> <a class="ms-fontColor-white" href="https://github.com/OfficeDev/PnP-JS-Core/wiki">Get Started</a></span>            
        </div>
        <div class="ms-Grid-col ms-u-md1 ms-u-lg2 ms-u-xl2 ms-u-hiddenSmDown">
            <span class="ms-font-xxl ms-fontColor-white"><i class="ms-Icon ms-Icon--story" aria-hidden="true"></i> <a class="ms-fontColor-white" href="https://github.com/OfficeDev/PnP-JS-Core/wiki">Wiki</a></span>            
        </div>
        <div class="ms-Grid-col ms-u-md1 ms-u-lg2 ms-u-xl3 ms-u-xxl2 ms-u-hiddenSmDown">
            <span class="ms-font-xxl ms-fontColor-white"><i class="ms-Icon ms-Icon--socialListening" aria-hidden="true">&nbsp;</i><a  class="ms-fontColor-white" href="https://twitter.com/hashtag/PnPJSCore">#PnPJSCore</a></span>            
        </div>
    </div>
    <div class="ms-Grid-row ms-bgColor-white">
        <div class="ms-Grid-col ms-u-md12 ms-u-hiddenSm">
            <div class="ms-Breadcrumb">
                <div class="ms-Breadcrumb-overflow">
                    <div class="ms-Breadcrumb-overflowButton ms-Icon ms-Icon--ellipsis" tabindex="1"></div>
                    <i class="ms-Breadcrumb-chevron ms-Icon ms-Icon--chevronRight"></i>
                    <div class="ms-Breadcrumb-overflowMenu">
                        <ul class="ms-ContextualMenu is-open"></ul>
                    </div>
                </div>
                <ul class="ms-Breadcrumb-list">
                    {{#with model}}{{> breadcrumb}}{{/with}}
                </ul>
            </div>
        </div>
    </div>
</header>
    

